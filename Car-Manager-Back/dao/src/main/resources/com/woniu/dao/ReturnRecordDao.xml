<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniu.dao.ReturnRecordDao">

    <select id="findList" resultType="ReturnRecordPo">
        select * from return_record
    </select>

    <!--新增出车记录-->
    <insert id="add" parameterType="ReturnRecordPo">
        insert into return_record(inTime,returnMileage,stopPosition,remarks)
        values(#{inTime},#{returnMileage},#{stopPosition},#{remarks})
    </insert>




</mapper>